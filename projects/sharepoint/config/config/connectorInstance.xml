<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="sharepoint-connector"
		class="com.google.enterprise.connector.sharepoint.SharepointConnector">
      <constructor-arg index="0" value="${sharepointUrl}"/>
      <constructor-arg index="1" value="${domain}"/>
      <constructor-arg index="2" value="${username}"/>
      <constructor-arg index="3" value="${Password}"/>
      <constructor-arg index="4" value="${googleConnectorWorkDir}"/>
      <constructor-arg index="5" value="${includedURls}"/>
      <constructor-arg index="6" value="${excludedURls}"/>
      <constructor-arg index="7" value="${mySiteBaseURL}"/>
      <constructor-arg index="8" value="${aliasHostName}"/>
      <constructor-arg index="9" value="${aliasPort}"/>
      <constructor-arg index="10" value="${SPType}"/>
	  <property name="whiteList">
			<list>
				<value>.*vti_title$</value>
				<value>.*vti_author$</value>
			</list>
		</property>
		<property name="blackList">
			<list>
				<value>.*vti_cachedcustomprops$</value>
				<value>.*vti_parserversion$</value>
				<value>.*ContentType$</value>
				<value>.*vti_cachedtitle$</value>
				<value>.*ContentTypeId$</value>
				<value>.*DocIcon$</value>
				<value>.*vti_cachedhastheme$</value>
				<value>.*vti_metatags$</value>
				<value>.*vti_charset$</value>
				<value>.*vti_cachedbodystyle$</value>
				<value>.*vti_cachedneedsrewrite$</value>
			</list>
		</property>
		<property name="FQDNConversion"><value>false</value></property>
	</bean>
</beans>
