<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
  <bean id="sharepoint-connector"
    class="com.google.enterprise.connector.sharepoint.spiimpl.SharepointConnectorType">
    <property name="configKeys">
      <list>
    <value>sharepointUrl</value>
    <value>kdcserver</value>
        <value>domain</value>
        <value>username</value>
        <value>password</value>
        <value>mySiteBaseURL</value>
        <value>includedURls</value>
        <value>excludedURls</value>
        <value>aliasMap</value>
        <value>sharepointCrawlingOptions</value>
        <value>authorization</value>
        <!-- Push ACLs configuration -->
        <value>pushAcls</value>
        <value>usernameFormatInAce</value>
        <value>groupnameFormatInAce</value>
        <!-- ldap configuration -->
        <value>ldapServerHostAddress</value>
        <value>ldapUserName</value>
        <value>ldapPassword</value>
        <value>ldapDomain</value>
        <value>portNumber</value>
        <value>searchBase</value>
        <value>authenticationType</value>
        <value>connectMethod</value>
        <!--  LDAP user groups membership cache settings -->
        <value>useCacheToStoreLdapUserGroupsMembership</value>
        <value>initialCacheSize</value>
        <value>cacheRefreshInterval</value>
      </list>
    </property>
  </bean>
</beans>
